#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int a,sum =1;
    scanf("%d",&a);
    if(a<0)
        printf("Invalid Input");
    else
    {
        for(int i=a;i>0;i--)
        {
            sum*=i;
        }
        printf("The Factorial of %d is %d",a,sum);
    }
    return 0;
}
