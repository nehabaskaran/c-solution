#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int a,b;
    scanf("%d\n%d\n",&a,&b);
    printf("Required length is %d.00m\nRequired quantity of carpet is %d.00sqm",((a+a)+(b+b)),a*b);
    return 0;
}
