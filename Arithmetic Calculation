#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    int a,b;
    scanf("%d\n%d\n",&a,&b);
    printf("Addition=%d\nSubtraction=%d\nMultiplication=%d\nDivision=%d\nModulo=%d\n",a+b,a-b,a*b,a/b,a%b);
    return 0;
}
