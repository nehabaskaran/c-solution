#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int n,f,c=0;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d ",&a[i]);
        
    }
    scanf("%d",&f);
    for(int j=0;j<n;j++)
    {
        if(a[j]==f)
        {
            c++;
        }
    }
    if(c)
    {
        printf("%d is presented in an array.",f);
    }
    else
        printf("%d is not presented in an array.",f);
    return 0;
}
