#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int a;
    scanf("%d",&a);
    int b=6,c=3;
    for(int i=1;i<=a;i++)
    {
        printf("%d ",b);
        b=b+c;
        c=c+2;
    }
    return 0;
}
