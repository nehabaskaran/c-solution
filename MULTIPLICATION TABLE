#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    int a;
    scanf("%d\n",&a);
    for(int i=1;i<=a;i++)
    {
        printf("%d * %d = %d\n",a,i,a*i);
    }
    return 0;
}
