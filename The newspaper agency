#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int w,x,y;
    scanf("%d\n%d\n%d\n",&w,&x,&y);
    int a=((w*x)-(w*y))-100;
    printf("%d",a);
    return 0;
}
