#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    int n,k;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    scanf("%d",&k);
    for(int j=0;j<n-1;j++)
    {
        for(int l=j+1;l<n;l++)
        {
            if(a[j]>a[l]){
                int temp=a[j];
                 a[j]=a[l];
            a[l]=temp;}
        }
    }
    printf("The elements are in the order: ");
     for(int i=0;i<n;i++)
    {
        printf("%d ",a[i]);
    }
    printf("\nThe Kth value is %d and Largest elements are ",k);
     
    int q=0;
    for(int i=k;i>0;i--)
    {
        printf("%d ",a[n-q-1]);
        q++;
    }
    return 0;
}
