#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    char a[50];
    int b,c;
    scanf("%s\n%d\n%d\n",a,&b,&c);
    if((b==1&&c>=70)||((b==1||b==2) && c>=75))
    {
        printf("Name of the Student:%s\n%s is Eligible for Placement",a,a);
    }
    else{
        printf("Name of the Student:%s\n%s is Not Eligible for Placement",a,a);
    }
    return 0;
}
