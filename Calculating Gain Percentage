#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>


int main() {
    float A, B, C;

    // Input: scooter price, repair cost, and selling price
    scanf("%f", &A);
    scanf("%f", &B);
    scanf("%f", &C);

    // Calculate cost price and gain
    float cost_price = A + B;
    float gain = C - cost_price;
    float gain_percent = (gain / cost_price) * 100;

    // Print result rounded to two decimal places
    printf("%.2f\n", gain_percent);

    return 0;
}
