#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    
    int n;
    long long b=1;
    int s=0;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d ",&a[i]);
        b*=a[i];
        s+=a[i];
    }
    
    float ttt=(float)s/n;
    float t=(int)(ttt*1000.0)/1000.0;
    
    printf("Product of the array:%.2f\nMean of the array:%.3f",(float)b,t);
    return 0;
}
