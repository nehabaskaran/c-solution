#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    int a;
    scanf("%d",&a);
    if(a>0)
    {
        printf("The Given Number %d is Positive.",a);
        
    }
    else if(a<0)
    {
        printf("The Given Number %d is Negative.",a);
    }
    else{
        printf("The Given Number %d is Zero.",a);
    }
    return 0;
}
