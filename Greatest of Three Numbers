#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    float a,b,c;
    scanf("%f\n%f\n%f\n",&a,&b,&c);
    if(a>b&&a>c)
        printf("Number1 is maximum with value of %.1f",a);
    else if(b>a&&b>c)
         printf("Number2 is maximum with value of %.1f",b);
    else
         printf("Number3 is maximum with value of %.1f",c);
        
    return 0;
}
