#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int a,b;
    scanf("%d\n%d\n",&a,&b);
    printf("%d\n%d\n",((a+a)+(b+b)),a*b);
    return 0;
}
